<script>
const sliderContainer = document.querySelector('.slider-container');
const sliderItems = document.querySelectorAll('.slider-item');
const sliderLinks = document.querySelectorAll('.slider-link');

let currentIndex = 0;

function showSlide(index) {
  sliderItems.forEach((item, i) => {
    item.classList.remove('active', 'previous', 'next');

    if (i === index) {
      item.classList.add('active');
      sliderLinks[i].classList.add('active');
    } else if (i === index - 1 || (index === 0 && i === sliderItems.length - 1)) {
      item.classList.add('previous');
      sliderLinks[i].classList.remove('active');
    } else if (i === index + 1 || (index === sliderItems.length - 1 && i === 0)) {
      item.classList.add('next');
      sliderLinks[i].classList.remove('active');
    }
  });
}

function nextSlide() {
  currentIndex = (currentIndex + 1) % sliderItems.length;
  showSlide(currentIndex);
}

function previousSlide() {
  currentIndex = (currentIndex - 1 + sliderItems.length) % sliderItems.length;
  showSlide(currentIndex);
}

sliderLinks.forEach((link, index) => {
  link.addEventListener('click', () => {
    showSlide(index);
  });
});

setInterval(nextSlide, 5000); // Change slide every 5 seconds


</script>
